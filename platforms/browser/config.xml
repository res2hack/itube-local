<?xml version='1.0' encoding='utf-8'?>
<widget id="m.m" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>iTube</name>
    <description>
Aplikasi local video server seperti Youtube.
    </description>
    <author email="restu.gandhi@gmail.com" href="http://m.m">
        Restu Gandhi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
